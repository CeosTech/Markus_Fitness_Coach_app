{
  "name": "ai-fitness-form-coach",
  "version": "1.0.0",
  "description": "Full-stack AI Fitness Coach application",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:server\" \"npm:dev:client\"",
    "dev:client": "vite",
    "dev:server": "node server.js",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server.js",
    "cap:init": "capacitor init ai-fitness-form-coach com.markus.fitnesscoach --web-dir=dist",
    "cap:sync": "capacitor sync",
    "cap:ios": "capacitor open ios",
    "cap:android": "capacitor open android",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/ios": "^7.4.4",
    "@google/genai": "^1.29.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "google-auth-library": "^9.0.0",
    "jspdf": "^2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-qr-code": "^2.0.12",
    "recharts": "^3.4.1",
    "sqlite3": "^5.1.7",
    "three": "^0.181.0"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.10.0",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.3",
    "@types/supertest": "^6.0.2",
    "@types/three": "^0.169.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.0.1",
    "postcss": "^8.4.38",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.2",
    "vite": "^5.4.10",
    "vitest": "^1.5.0"
  }
}
